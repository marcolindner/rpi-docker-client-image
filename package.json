    "type": "git",
    "url": "https://github.com/marcolindner/rpi-docker-client-image.git"
  },
  "author": "Marco Lindner",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/marcolindner/rpi-docker-client-image/issues"
  },
  "dependencies": {
    "bootstrap": "^3.3.7",
    "copy": "^0.3.0",
    "express": "^4.14.0"
  }
}
